{
  "name": "portfolio",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write .",
    "ci:lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "ci:lint:prettier": "prettier --check .",
    "ci:lint:ts": "tsc --noEmit",
    "deploy:dev": "cdk deploy -c stage=dev",
    "deploy:prod": "cdk deploy -c stage=prod",
    "destroy:dev": "cdk destroy -c stage=dev",
    "destroy:prod": "cdk destroy -c stage=prod",
    "diff:dev": "cdk diff -c stage=dev",
    "diff:prod": "cdk diff -c stage=prod",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:cdk",
    "lint:cdk": "npm run synth:dev",
    "lint:eslint": "eslint",
    "lint:eslint:fix": "eslint --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:ts": "tsc --noEmit",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "synth:dev": "cdk synth -c stage=dev",
    "synth:prod": "cdk synth -c stage=prod",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/dotenv": "^6.1.1",
    "@eslint/js": "^9.16.0",
    "@types/node": "^22.5.4",
    "aws-cdk": "^2.1006.0",
    "aws-cdk-lib": "^2.195.0",
    "constructs": "^10.3.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.13.0",
    "prettier": "^3.4.2",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.18.0"
  },
  "dependencies": {
    "@aws-sdk/client-acm": "^3.650.0",
    "@aws-sdk/client-route-53": "^3.817.0",
    "@aws-sdk/client-sts": "^3.650.0"
  }
}
